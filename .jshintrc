{
	"bitwise": true,
	"camelcase": true,
	"curly": true,
	"forin": true,
	"eqeqeq": true,
	"freeze": true,
	"immed": true,
	"latedef": true,
	"indent": 4,
	"newcap": true,
	"noarg": true,
	"noempty": true,
	"nonbsp": true,
	"nonew": true,
	"quotmark": "single",
	"maxdepth": 3,
	"maxstatements": 15,
	"maxcomplexity": 6,
	"eqnull": true,
	"funcscope": true,
	"undef": true,
	"unused": "vars",
	"strict": true,
	"maxparams": 3,

	// Environments (Note we don't use all these all the time so this is a little overly lax)
	"node": true,
	"browser": true,
	"jquery": true,
	"devel": true // TODO: Get rid of this one and validate without devel tools once we go to production
}
